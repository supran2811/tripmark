{
  "name": "tripmark",
  "version": "0.0.1",
  "description": "Bookmark your trips",
  "main": "index.js",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "test": "jest",
    "dev": "cross-env node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-func": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "copy-routes": "cpx \"src/routes.js\" \"dist/functions\" -C",
    "copy-i18n": "cpx \"src/i18n.js\" \"dist/functions\" -C",
    "build-i18n":"cpx \"src/app/locales/**/*.*\" \"dist/functions/next/locales\" -C",
    "copy-deps": "cpx \"*{package.json,package-lock.json}\" \"dist/functions\"",
    "install-deps": "cd \"dist/functions\" && npm i",
    "build-app": "next build \"src/app\"",
    "deploy-all": "cross-env NODE_ENV=production firebase deploy",
    "deploy-func": "firebase deploy --only functions",
    "deploy-app": "firebase deploy --only hosting,functions:app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supran2811/tripmark.git"
  },
  "keywords": [
    "Bookmark",
    "trip"
  ],
